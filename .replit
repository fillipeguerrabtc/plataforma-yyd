modules = ["python-3.11", "nodejs-20", "postgresql-16", "python3"]
[agent]
expertMode = true
integrations = ["javascript_stripe:1.0.0", "twilio:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["cargo", "libiconv", "libxcrypt", "postgresql", "rustc"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Client"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Backoffice"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Aurora"

[[workflows.workflow]]
name = "Client"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd yyd/apps/client && npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow]]
name = "Backoffice"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd yyd/apps/backoffice && npm run dev"
waitForPort = 3001

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow]]
name = "Aurora"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd yyd/aurora && python main.py"
waitForPort = 8008

